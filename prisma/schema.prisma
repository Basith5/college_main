// This is your Prisma schema file.

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model department {
  id             Int    @id @default(autoincrement())
  departmentCode String
  name           String
  catagory       String @default("Arts")
  year           Int
  codes          code[]
}

model code {
  id         Int        @id @default(autoincrement())
  code       String
  name       String
  depID      Int
  department department @relation(fields: [depID], references: [id])
  students   student[]
  staff      staff[]
  pso        PSO[]
}

model staff {
  id        Int    @id @default(autoincrement())
  uname     String
  staffName String
  codeId    Int
  code      code   @relation(fields: [codeId], references: [id])
}

model student {
  id      Int     @id @default(autoincrement())
  regNo   String
  claass  String
  section String
  marks   marks[]
  codeId  Int
  code    code    @relation(fields: [codeId], references: [id])
}

model marks {
  id        Int     @id @default(autoincrement())
  C1LOT     Float?
  C1MOT     Float?
  C1HOT     Float?
  C2LOT     Float?
  C2MOT     Float?
  C2HOT     Float?
  ASG1      Float?
  ASG2      Float?
  ASGCO1    Float?
  ASGCO2    Float?
  ESELOT    Float?
  ESEMOT    Float?
  ESEHOT    Float?
  TLOT      Float?
  TMOT      Float?
  THOT      Float?
  C1STATUS  String?
  C2STATUS  String?
  ESESTATUS String?
  C1STAFF   String?
  C2STAFF   String?
  ESESTAFF  String?
  ASG1STAFF String?
  ASG2STAFF String?
  studentId Int
  student   student @relation(fields: [studentId], references: [id])
}

model PSO {
  id      Int  @id @default(autoincrement())
  PSO1CO1 Int
  PSO1CO2 Int
  PSO1CO3 Int
  PSO1CO4 Int
  PSO1CO5 Int
  PSO2CO1 Int
  PSO2CO2 Int
  PSO2CO3 Int
  PSO2CO4 Int
  PSO2CO5 Int
  PSO3CO1 Int
  PSO3CO2 Int
  PSO3CO3 Int
  PSO3CO4 Int
  PSO3CO5 Int
  PSO4CO1 Int
  PSO4CO2 Int
  PSO4CO3 Int
  PSO4CO4 Int
  PSO4CO5 Int
  PSO5CO1 Int
  PSO5CO2 Int
  PSO5CO3 Int
  PSO5CO4 Int
  PSO5CO5 Int
  codeId  Int
  code    code @relation(fields: [codeId], references: [id])
}

model User {
  id       Int    @id @default(autoincrement())
  uname    String
  password String
  role     Role   @default(Staff)
  name     String @default("none")
}

enum Role {
  Admin
  Staff
}

model date {
  id   Int @id @default(autoincrement())
  date Int
}
